	doi = {https://doi.org/10.1016/j.biocon.2013.12.011},
	issn = {0006-3207},
	journal = {Biological Conservation},
	keywords = {Body mass, Database, Minimum viable population, Population viability analysis, Time horizon, Review},
	pages = {92 - 102},
	title = {Toward better application of minimum area requirements in conservation planning},
	url = {http://www.sciencedirect.com/science/article/pii/S0006320713004321},
	volume = {170},
	year = {2014},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S0006320713004321},
	bdsk-url-2 = {https://doi.org/10.1016/j.biocon.2013.12.011}}

